{
  "name": "archestra",
  "version": "0.0.1",
  "description": "Record all your interactions with Claude Code as you develop your projects",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "proxy:dev": "nodemon --exec \"tsc && node dist/main.js\" --watch src --ext ts",
    "dev": "tsc --watch --preserveWatchOutput",
    "cli-chat-with-guardrails": "tsx src/guardrails/chat.ts",
    "cli-chat-with-middleware": "tsx src/guardrails/chat-with-middleware.ts",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "keywords": [
    "archestra",
    "nodejs",
    "interceptor"
  ],
  "author": "Archestra",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/http-proxy": "^1.17.16",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.0.0",
    "nodemon": "^3.1.10",
    "pino-pretty": "^13.1.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.0"
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.17",
    "@ai-sdk/openai": "2.1.0-beta.5",
    "@ai-sdk/provider": "^2.0.0",
    "@fastify/http-proxy": "^11.3.0",
    "ai": "5.1.0-beta.12",
    "dotenv": "^17.2.2",
    "fastify": "^5.6.1",
    "lodash": "^4.17.21",
    "openai": "^5.23.0",
    "uuid": "^13.0.0",
    "zod": "^4.1.11"
  }
}
