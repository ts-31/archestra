{
  "name": "@backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "start": "node dist/server.js",
    "dev": "tsup --watch --onSuccess \"node dist/server.js\"",
    "dev:debug": "tsup --watch --onSuccess \"node --inspect-brk dist/server.js\"",
    "test": "vitest",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "lint:fix:unsafe": "biome check --write --unsafe",
    "check:ci": "pnpm type-check && pnpm test && biome ci",
    "type-check": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx --tsconfig standalone-scripts.tsconfig.json src/standalone-scripts/seed-db.ts",
    "db:clean": "tsx --tsconfig standalone-scripts.tsconfig.json src/standalone-scripts/clean-db-on-dev.ts",
    "db:seed:mock-data": "tsx --tsconfig standalone-scripts.tsconfig.json src/standalone-scripts/seed-mock-data.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.66.0",
    "@fastify/cors": "^11.1.0",
    "@fastify/http-proxy": "^11.3.0",
    "@fastify/swagger": "^9.5.2",
    "@google/genai": "^1.24.0",
    "@shared": "workspace:*",
    "ai": "^5.0.57",
    "better-auth": "^1.3.27",
    "dotenv": "^17.2.2",
    "drizzle-kit": "^0.31.5",
    "drizzle-orm": "^0.44.5",
    "drizzle-zod": "^0.8.3",
    "fastify": "^5.6.1",
    "fastify-plugin": "^5.1.0",
    "fastify-type-provider-zod": "^6.0.0",
    "lodash": "^4.17.21",
    "openai": "^6.0.0",
    "pg": "^8.16.3",
    "pino-pretty": "^13.1.1",
    "uuid": "^13.0.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@electric-sql/pglite": "^0.3.10",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.15.5",
    "@types/uuid": "^11.0.0",
    "tsc-alias": "^1.8.10",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.0",
    "vitest": "^3.2.4"
  }
}
